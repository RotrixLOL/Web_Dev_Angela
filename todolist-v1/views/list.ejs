<%- include('header') -%>
<div class="box" id="heading">
	<h1>Lista del <%= listTitle%></h1>
</div>

<div class="box">
		<% for( let i = 0; i < newItems.length; i++ ) { %>
			<div class="item">
				<input type="checkbox">
				<p><%= newItems[i] %></p>
			</div>
		<% } %>

		<form class="item" action="/" method="post">
			<input type="text" name="newItem" placeholder="Item nuevo" autocomplete="off" required>
			<button type="submit" name="list" value=<%= listTitle %>>ï¼‹</button>
		</form>
</div>
<%- include('footer') -%>